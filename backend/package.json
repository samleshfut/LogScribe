{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/handler.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "logs:bug": "serverless logs -f analyzeBug --tail",
    "logs:lint": "serverless logs -f lintCode --tail",
    "logs:review": "serverless logs -f reviewPR --tail"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "bedrock",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.2",
    "nodemon": "^3.1.0",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.52.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.873.0",
    "@aws-sdk/client-bedrock-runtime": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0"
  }
}
